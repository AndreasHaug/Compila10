(PROGRAM NAME:euclid begin
  (PROC_DECL NAME:gcd
    (PARAMFIELD_DECL NAME:a TYPE:int)
    (PARAMFIELD_DECL NAME:b TYPE:int)
  TYPE:int
  begin
    (VAR_DECL NAME:res TYPE:int)
  in
    (IF_STMT (= NAME:a Intliteral:0)
      (ASSIGN_STMT NAME:res NAME:b)
      (else
        (WHILE_STMT (<> NAME:b Intliteral:0)
          (IF_STMT (> NAME:a NAME:b)
            (ASSIGN_STMT NAME:a (- NAME:a NAME:b))
            (else
              (ASSIGN_STMT NAME:b (- NAME:b NAME:a)))))
        (ASSIGN_STMT NAME:res NAME:a)))
    (RETURN_STMT NAME:res)
  end)
  (PROC_DECL NAME:main begin in
    (CALL_STMT NAME:printint
        (CALL_STMT NAME:gcd Intliteral:6 Intliteral:19))
    (CALL_STMT NAME:printline Stringliteral:"")
    (CALL_STMT NAME:printint
        (CALL_STMT NAME:gcd Intliteral:6 Intliteral:9))
    (CALL_STMT NAME:printline Stringliteral:"")
    (CALL_STMT NAME:printint
        (CALL_STMT NAME:gcd Intliteral:629 Intliteral:592))
    (CALL_STMT NAME:printline Stringliteral:"")
  end)
end)